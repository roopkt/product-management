{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../out-tsc/spec",
    "target": "es5",
    "module": "commonjs",
    "baseUrl": "",
    "rootDir": "./",
    "strict": true,
    "paths": {},
    "strictNullChecks": false
  },
  "exclude": [
    "node_modules",
    "**/*/node_modules"
  ],
  "include": [
    "**/*.spec.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}